# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'FLEX' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FLEX

  # Flipboard Explorer
  pod 'FLEX'

  # Swift auto layout
  pod 'SnapKit'

  # Swift initializer
  pod 'Then'

  # Utility
  pod 'SwiftyUserDefaults'

  # Rx
  pod 'RxSwift'
  pod 'RxFlow'

  # Dependency Injection
  pod 'Pure'

  # Layout
  pod 'Neon'

   # Resources
  pod 'R.swift'

end
